{
 "compilerOptions": {
   "target": "esnext",
   "lib": [
     "dom",
     "dom.iterable",
     "esnext"
   ],
   "allowJs": true,
   "skipLibCheck": true,
   "sourceMap": true,
   "outDir": "dist",
   "strict": true,
   "esModuleInterop": true,
   "forceConsistentCasingInFileNames": true,
   "noEmit": true,
   "module": "commonjs",
   "moduleResolution": "node",
   "resolveJsonModule": true,
   "isolatedModules": true,
   "jsx": "preserve"
 },
 "exclude": [
   "node_modules"
 ]
}